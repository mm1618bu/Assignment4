number = int(input("Enter your card number: "))
def isValid(number):
    y = len(str(number))
    y1 = str(number)
    z = list(y1.strip(" "))
    typ3 = int(z[0])
    print(typ3)
    if y < 13 or y > 16:
        print("fail")
    else:
        if typ3 == 4:
            print("visa")
        elif typ3 == 5:
            print("MasterCard")
        elif typ3 == 3:
            print("AmericanExpress")
        elif typ3 == 6:
            print("Discover")
        else:
            print("no")

