unitedStates = [["Massachusetts","Boston"],["New York","Albany"],["",""],["",""]]
successful = 0
for i in range(len(unitedStates)):
    cap = input("What is the capital of "+unitedStates[i][0]+"? ")
    if cap.lower == unitedStates[i][1].lower:
        successful += 1
        print("Correct")
    else:
        print("Wrong")
print(successful)
